{
  "name": "@ourchitecture/oclif-cli",
  "description": "Ourchitecture CLI",
  "version": "0.0.1",
  "license": "Unlicense",
  "author": "Eric Swanson @ericis",
  "homepage": "https://github.com/ourchitecture/patterns-and-tutorials",
  "bugs": "https://github.com/ourchitecture/patterns-and-tutorials/issues",
  "repository": "ourchitecture/patterns-and-tutorials",
  "keywords": [
    "oclif"
  ],
  "bin": {
    "our": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "our",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  }
}
